package fr.isika.cdi9.livraison.bean;

public class Commande {

	private Fournissseur fournisseur;
	private Client client;

	public void passerUneCommande() {
		System.out.println("Merci d'avoir effectuÃ© la commande Mr " + client.getNomClient());
		System.out.println("Le fournisseur :" + fournisseur.getNomFournisseur() + " sera averti de cette derniÃ¨re; ");
	}

	public Fournissseur getFournisseur() {
		return fournisseur;
	}

	public void setFournisseur(Fournissseur fournisseur) {
		this.fournisseur = fournisseur;
	}

	public Client getClient() {
		return client;
	}

	public void setClient(Client client) {
		this.client = client;
	}

}
